{
  "name": "propally-notification-service",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "type": "commonjs",
  "devDependencies": {
    "@types/node": "^16.11.10",
    "rimraf": "^3.0.2",
    "ts-node": "10.7.0",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.11",
    "@types/joi": "17.2.3",
    "@types/jsonwebtoken": "8.5.0",
    "assemblyai": "^3.1.3",
    "axios": "^1.6.0",
    "bcryptjs": "2.4.3",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cloudinary": "1.30.0",
    "cookie-parser": "1.4.6",
    "redis": "^4.3.0",
    "cors": "2.8.5",
    "cross-env": "7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "16.3.1",
    "express": "4.18.2",
    "express-validator": "7.0.1",
    "flutterwave-node-v3": "^1.1.7",
    "google-auth-library": "^8.7.0",
    "google-libphonenumber": "^3.2.32",
    "jest": "^29.7.0",
    "joi": "17.4.0",
    "jsonwebtoken": "8.5.1",
    "multer": "1.4.4",
    "nodemailer": "^6.9.7",
    "nodemon": "^3.0.1",
    "pg": "^8.4.0",
    "prettier": "3.0.2",
    "reflect-metadata": "^0.1.13",
    "sinon": "^17.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typeorm": "^0.3.0-rc.21",
    "typeorm-seeding": "^1.6.1",
    "zod": "3.21.4"
  },
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "test-dev": "ts-node ./okemute.ts",
    "start:dev": "nodemon",
    "build:dev": "rimraf ./build && tsc",
    "start": "node ./build/bin/www.js",
    "debug:dev": "cross-env NODE_ENV=development nodemon --inspect ",
    "typeorm": "typeorm-ts-node-commonjs",
    "show:migrations": "npm run typeorm migration:show -- -d src/lib/infra/db/typeOrm.config.ts",
    "run:migrations": "npm run typeorm migration:run -- -d src/lib/infra/db/typeOrm.config.ts",
    "generate:migration": "npm run typeorm --  -d ./src/lib/infra/db/typeOrm.config.ts  migration:generate  ./src/lib/infra/db/migrations/$npm_config_name ",
    "typeorm:create-migration": "npm run typeorm migration:create ./src/lib/infra/db/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/lib/infra/db/typeOrm.config.ts migration:revert",
    "prettier": "npx prettier src test --write",
    "prettier:format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "test": "jest",
    "test:dev": "jest --watchAll",
    "testWithCoverage": "jest --coverage",
    "seed": "node ./node_modules/typeorm-seeding/dist/cli.js seed"
    
  }
}